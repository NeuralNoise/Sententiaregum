{
    "name": "sententiaregum/sententiaregum",
    "license": "GPL-2.0",
    "type": "project",
    "description": "A social network based on Symfony2 and ReactJS",
    "autoload": {
        "psr-4": { "": "src/" }
    },
    "require": {
        "php": "^5.6|~7.0",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "~1.5",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "wjzijderveld/check-bundles": "~1.1",
        "ma27/api-key-authentication-bundle": "~0.1",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "twig/extensions": "~1.3",
        "jms/di-extra-bundle": "~1.6",
        "friendsofsymfony/rest-bundle": "~1.7",
        "jms/serializer-bundle": "~0.13",
        "sonata-project/notification-bundle": "~2.3",
        "lunetics/locale-bundle": "~2.4",
        "snc/redis-bundle": "~1.1",
        "predis/predis": "~1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "phpunit/phpunit": "~4.8",
        "fabpot/php-cs-fixer": "~1.10",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0"
    },
    "scripts": {
        "pre-install-cmd": [
            "AppBundle\\Composer\\PreInstallHandler::determineFirstInstall"
        ],
        "post-install-cmd": [
            "WillemJan\\CheckBundles\\Composer\\CheckBundles::postPackageUpdate",
            "AppBundle\\Composer\\ScriptHandler::installNpmDependencies",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "AppBundle\\Composer\\ScriptHandler::createDoctrineSchema",
            "AppBundle\\Composer\\ScriptHandler::loadDoctrineDataFixtures"
        ],
        "post-update-cmd": [
            "WillemJan\\CheckBundles\\Composer\\CheckBundles::postPackageUpdate",
            "AppBundle\\Composer\\ScriptHandler::installNpmDependencies",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "AppBundle\\Composer\\ScriptHandler::updateDoctrineSchema"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "authors": [
        {
            "name": "Maximilian Bosch",
            "email": "maximilian.bosch.27@gmail.com",
            "homepage": "https://github.com/Ma27",
            "role": "Project Lead"
        },
        {
            "name": "Ben Bieler",
            "email": "benjaminbieler2014@gmail.com",
            "homepage": "https://github.com/benbieler",
            "role": "Core Developer"
        }
    ]
}
